<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from IfServiceService WSDL service -->

  <service name="IfService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ifservice.uge.fr"/>
      <parameter name="wsdlServiceElement" value="IfServiceService"/>
      <parameter name="schemaQualified" value="http://ifservice.uge.fr"/>
      <parameter name="wsdlServicePort" value="IfService"/>
      <parameter name="className" value="fr.uge.ifservice.IfServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="IfService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getSoldProductsList" qname="operNS:getSoldProductsList" xmlns:operNS="http://ifservice.uge.fr" returnQName="retNS:getSoldProductsListReturn" xmlns:retNS="http://ifservice.uge.fr" returnType="rtns:anyType" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="buyProduct" qname="operNS:buyProduct" xmlns:operNS="http://ifservice.uge.fr" returnQName="retNS:buyProductReturn" xmlns:retNS="http://ifservice.uge.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:ad" xmlns:pns="http://ifservice.uge.fr" type="tns:anyType" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:client" xmlns:pns="http://ifservice.uge.fr" type="tns:anyType" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getSoldProductsList buyProduct"/>

      <arrayMapping
        xmlns:ns="http://ifservice.uge.fr"
        qname="ns:>getSoldProductsListResponse"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ifservice.uge.fr"
        qname="ns:>buyProduct"
        type="java:fr.uge.ifservice.BuyProduct"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ifservice.uge.fr"
        qname="ns:>buyProductResponse"
        type="java:fr.uge.ifservice.BuyProductResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://ifservice.uge.fr"
        qname="ns:>getSoldProductsList"
        type="java:fr.uge.ifservice.GetSoldProductsList"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
