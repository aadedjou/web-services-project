<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ConverterService WSDL service -->

  <service name="Converter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Converter"/>
      <parameter name="wsdlServiceElement" value="ConverterService"/>
      <parameter name="schemaQualified" value="http://Converter"/>
      <parameter name="wsdlServicePort" value="Converter"/>
      <parameter name="className" value="Converter_pkg.ConverterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Converter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convert" qname="operNS:convert" xmlns:operNS="http://Converter" returnQName="retNS:convertReturn" xmlns:retNS="http://Converter" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:currentCurrency" xmlns:pns="http://Converter" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:targetCurrency" xmlns:pns="http://Converter" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:amount" xmlns:pns="http://Converter" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="convert"/>

  </service>
</deployment>
